# Redis Configuration
REDIS_URL=redis://localhost:6379

# FIBO API Provider Configuration
# Options: 'bria' (default) or 'fal'
FIBO_API_PROVIDER=bria

# Bria API Configuration (if using FIBO_API_PROVIDER=bria)
BRIA_API_TOKEN=your_bria_api_token_here
# Legacy variable name (also supported)
FIBO_API_KEY=your_bria_api_token_here

# Fal.ai API Configuration (if using FIBO_API_PROVIDER=fal)
FAL_KEY=your_fal_api_key_here

# Model Configuration
MODEL_VERSION=FIBO

# FIBO Generation Parameters
FIBO_STEPS=50
FIBO_GUIDANCE_SCALE=5
FIBO_ASPECT_RATIO=1:1

# Storage Configuration
STORAGE_DIR=./storage

# Web Server Configuration
WEB_PORT=3000
