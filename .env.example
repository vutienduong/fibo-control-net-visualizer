# Redis Configuration
REDIS_URL=redis://localhost:6379

# FIBO API Provider Configuration
# Options: 'bria' (default) or 'fal'
FIBO_API_PROVIDER=bria

# Bria API Configuration (if using FIBO_API_PROVIDER=bria)
BRIA_API_TOKEN=your_bria_api_token_here
# Legacy variable name (also supported)
FIBO_API_KEY=your_bria_api_token_here

# Fal.ai API Configuration (if using FIBO_API_PROVIDER=fal)
FAL_KEY=your_fal_api_key_here

# Model Configuration
MODEL_VERSION=FIBO

# FIBO Generation Parameters
FIBO_STEPS=50
FIBO_GUIDANCE_SCALE=5
FIBO_ASPECT_RATIO=1:1

# Storage Configuration
STORAGE_DIR=./storage

# Web Server Configuration
WEB_PORT=3000

# CDN Configuration
# Options: 'cloudfront' | 'cloudinary' | 'cloudflare' | 'custom' | 'none'
NEXT_PUBLIC_CDN_PROVIDER=none
NEXT_PUBLIC_CDN_BASE_URL=
NEXT_PUBLIC_CDN_FORMAT=webp
NEXT_PUBLIC_CDN_QUALITY=85

# Cloudinary specific (if using NEXT_PUBLIC_CDN_PROVIDER=cloudinary)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=

# Cloudflare specific (if using NEXT_PUBLIC_CDN_PROVIDER=cloudflare)
NEXT_PUBLIC_CLOUDFLARE_ACCOUNT_HASH=

# Caching Configuration
CACHE_MAX_AGE=86400
CACHE_STALE_WHILE_REVALIDATE=604800
